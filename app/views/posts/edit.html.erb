<h1>Edit Post</h1>
<%= form_for(@post) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %><br>
  <%= f.label :content %>
  <%= f.text_field :content %><br>
  <%= f.label :summary %>
  <%= f.text_field :summary %><br>
  <%= f.label :category %>
  <%= f.text_field :category %><br>
  <%= f.submit %>
<% end %>
